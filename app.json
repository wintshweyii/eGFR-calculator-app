{
  "expo": {
    "name": "eGFR Calculator App",
    "slug": "eGFR-Calculator-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/kidneys.png",
    "scheme": "egfrcalculatorapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kidney-logo.png",
        "backgroundColor": "#eaf3fb"
      },
      "predictiveBackGestureEnabled": false,
      "package": "com.wintshweyii.eGFRCalculatorApp"
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/kidney-logo.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/kidney-logo.png",
          "imageWidth": 180,
          "resizeMode": "contain",
          "backgroundColor": "#eaf3fb",
          "dark": {
            "backgroundColor": "#eaf3fb"
          }
        }
      ],
      "expo-font",
      "expo-sqlite",
      "@react-native-community/datetimepicker"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "5b786001-06bb-4482-b528-25eef0d18e87"
      }
    }
  }
}
